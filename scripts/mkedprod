#!/bin/sh

cmd=mkedprod
tmpl=EDProducer
tdir=`echo $0 | sed "s,/$cmd,,g"`
examples="Examples: \
    \n\t# generate My$tmpl package
    \n\t$cmd --name=My$tmpl
    \n\t# generate My$tmpl package with examples
    \n\t$cmd --name=My$tmpl --keep-etags=\"@example_particle\"
    "
source $tdir/mktmpl.sh
